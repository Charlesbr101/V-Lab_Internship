# Test-specific environment variables for MySQL used by docker-compose.test.yml
# Keep values minimal and avoid setting MYSQL_USER=root here to prevent entrypoint
# confusion. The container will be started with MYSQL_ROOT_PASSWORD and an initial DB.
MYSQL_ROOT_PASSWORD=password
MYSQL_HOST=127.0.0.1
MYSQL_TEST_DB=digital_library_test
MYSQL_TEST_PORT=3307
